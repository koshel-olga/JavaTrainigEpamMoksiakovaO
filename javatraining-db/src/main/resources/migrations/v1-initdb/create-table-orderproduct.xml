<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="2021-05-21_create_table_OrderProduct" author="moksiakova">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="OrderProduct"/>
            </not>
        </preConditions>
        <createTable tableName="OrderProduct">
            <column name="ProductId" type="int">
                <constraints foreignKeyName="fk_product_id" nullable="false"
                             referencedTableName="Product" referencedColumnNames="ProductId"/>
            </column>
            <column name="OrderId" type="int">
                <constraints foreignKeyName="fk_order_id" nullable="false"
                             referencedTableName="Order" referencedColumnNames="OrderId"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>